{
  "name": "intellpro-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "server-install": "cd server && npm install",
    "client-install": "cd client && npm install",
    "install-all-deps": "concurrently \"npm run server-install\" \"npm run client-install\"",
    "client": "cd client && npm run start",
    "server": "cd server && npm run serve",
    "project": "concurrently \"npm run client-run\" \"npm run server\"",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-all-deps && npm run build",
    "develop": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0"
  }
}
